<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" "http://www.springframework.org/dtd/spring-beans.dtd">

<beans>
    <bean
            id="executorService"
            class="java.util.concurrent.Executors"
            factory-method="newFixedThreadPool">
        <constructor-arg>
            <value>9</value>
        </constructor-arg>
    </bean>


</beans>